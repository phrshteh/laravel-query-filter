{
    "name": "phrshteh/laravel-query-filter",
    "description": "A laravel package for resource filtering via request query string",
    "homepage": "https://github.com/phrshteh/laravel-query-filter",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "query",
        "filter",
        "resource",
        "pagination",
        "model",
        "eloquent",
        "json"
    ],
    "authors": [
        {
            "name": "Omid Alizadeh",
            "email": "om.alizadeh1@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-json": "*",
        "illuminate/contracts": "^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0",
        "illuminate/console": "^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0",
        "illuminate/database": "^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0",
        "illuminate/http": "^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0",
        "illuminate/support": "^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0"
    },
    "autoload": {
        "files": [
            "helpers/functions.php"
        ],
        "psr-4": {
            "Omalizadeh\\QueryFilter\\": "src/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.8",
        "orchestra/testbench": "^6.24 || ^7.25 || ^8.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Omalizadeh\\QueryFilter\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Omalizadeh\\QueryFilter\\Providers\\QueryFilterServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit --color=always",
        "cs-fix": "./vendor/bin/php-cs-fixer fix -v --show-progress=dots"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
